<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="viewModel"
            type="com.odbpo.fenggou.feature.window.more.MoreViewModel" />
    </data>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/shape_fffff_backgd_5radius_rectangle"
        android:orientation="vertical">

        <android.support.v7.widget.AppCompatTextView
            style="@style/App.Theme.TextAppearance.ww.normal.black999"
            android:drawableLeft="@drawable/wode"
            android:drawablePadding="@dimen/app.padding.large12"
            android:padding="@dimen/app.padding.large12"
            android:text="@string/app.window.more" />

        <include layout="@layout/include_line_h" />

        <android.support.v7.widget.AppCompatTextView
            style="@style/App.Theme.TextAppearance.ww.normal.black999"
            android:drawableLeft="@drawable/zhuye"
            android:drawablePadding="@dimen/app.padding.large12"
            android:padding="@dimen/app.padding.large12"
            android:text="@string/app.window.more.main" />

        <include layout="@layout/include_line_h" />

        <android.support.v7.widget.AppCompatTextView
            style="@style/App.Theme.TextAppearance.ww.normal.black999"
            android:drawableLeft="@drawable/wodeshoucang"
            android:drawablePadding="@dimen/app.padding.large12"
            android:padding="@dimen/app.padding.large12"
            android:text="@string/app.window.more.collect" />

    </LinearLayout>
</layout>