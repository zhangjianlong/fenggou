<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">
    <data>
        <variable name="SearchNeedResultTabModel"
            type="com.slash.youth.ui.viewmodel.SearchNeedResultTabModel"/>
    </data>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <!--页签标题-->
     <FrameLayout
         android:id="@+id/Fl_tab"
         android:layout_width="match_parent"
         android:layout_height="wrap_content">

     </FrameLayout>
        <FrameLayout
            android:id="@+id/fl_showSearchResult"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <com.slash.youth.ui.view.RefreshListView
                android:id="@+id/lv_searchTab"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#f1f1f1"
               >
            </com.slash.youth.ui.view.RefreshListView>
        </FrameLayout>

    </LinearLayout>
</layout>